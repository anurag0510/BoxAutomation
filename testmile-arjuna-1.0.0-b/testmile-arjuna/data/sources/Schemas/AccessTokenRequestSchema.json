{
	"type":"object",
	"$schema": "http://json-schema.org/draft-03/schema",
	"required":false,
	"properties":{
		"access_token": {
			"type":"string",
			"required":true
		},
		"expires_in": {
			"type":"number",
			"required":true
		},
		"refresh_token": {
			"type":"string",
			"required":true
		},
		"restricted_to": {
			"type":"array",
			"required":false
		},
		"token_type": {
			"type":"string",
			"required":false
		}
	}
}